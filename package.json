{
  "name": "node-redis-web",
  "version": "1.0.0",
  "description": "Redis管理工具 - 基于Node.js和Vue3开发",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "build:server": "node build-server.js",
    "build:tgz": "node build-tgz.js",
    "build:web": "cd client && npm run build:web",
    "install-all": "npm install && cd client && npm install",
    "docs": "apidoc -i server/ -o docs/",
    "docs:serve": "apidoc -i server/ -o docs/ && http-server docs/ -p 8080"
  },
  "keywords": [
    "redis",
    "management",
    "node",
    "vue3"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "redis": "^4.6.10",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "apidoc": "^1.2.0",
    "concurrently": "^8.2.2",
    "esbuild": "^0.19.12",
    "nodemon": "^3.0.1"
  }
}
